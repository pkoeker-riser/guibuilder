xFiller.an=C
xFiller.fill=H

xFiller.type=GuiLabel

xFiller.ClassAttributes.Attrib=\
	ADD <$Import> de.guibuilder.framework.<type>\n\

xFiller.guiInitAttributes.Attrib=\
	ADD <$guiInitLocal> GuiLabel lbl

xFiller.guiInitAction.Attrib=\
	// xFiller\n\
	lbl = new <type>(" ");\n\
	? <x> SET <$X> <x>\n\
	? <y> SET <$Y> <y>\n\
	? <min> lbl.setMinimumSize(new Dimension(<min>, lbl.getMinimumSize().height));\n\
	? <max> lbl.setMaximumSize(new Dimension(<max>, lbl.getMaximumSize().height));\n\
	<$currentPanel>.add(lbl, new GridBagConstraints(<$X>, <$Y>, <w>, <h>, <wx>, <wy>\n\
      	\t ,<an>, <fill>, new Insets(<it>, <il>, <ib>, <ir>), <px>, <py>));\n\
	SET <$X> + <w>\n\
	IF <eol>\n\
		SET <$X> 0\n\
		SET <$Y> + <h>\n\
	END_IF\n\

